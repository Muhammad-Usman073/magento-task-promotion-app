<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">

        <virtualType name="Mgn\Notification\Model\ResourceModel\Notification\Grid\Collection" type="\Magento\Framework\View\Element\UiComponent\DataProvider\SearchResult">
            <arguments>
                <argument name="mainTable" xsi:type="string">my_database_table</argument>
                <argument name="resourceModel" xsi:type="string">Mgn\Notification\Model\ResourceModel\NotificationResource</argument>
            </arguments>
        </virtualType>
        <type name="Magento\Framework\View\Element\UiComponent\DataProvider\CollectionFactory">
            <arguments>
                <argument name="collections" xsi:type="array">
                    <item name="notification_table_listing_data_source" xsi:type="string">Mgn\Notification\Model\ResourceModel\Notification\Grid\Collection</item>
                </argument>
            </arguments>
        </type>

<preference for="Mgn\Notification\Api\Data\NotificationInterface" type="Mgn\Notification\Model\Notification" />
    <preference for="Mgn\Notification\Api\NotificationRepositoryInterface" type="Mgn\Notification\Model\NotificationRepository" />
    <preference for="Magento\Email\Model\Transport" type="Mgn\Notification\Mail\CustomEmail" />
</config>
